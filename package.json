{
  "name": "apollo-link-form-data",
  "version": "1.0.0",
  "description": "Send your GraphQL request with apollo-link as FormData or JSON.",
  "repository": "octet-stream/apollo-link-form-data",
  "author": "Nick K. <nick.kruchinin@gmail.com>",
  "license": "MIT",
  "main": "lib/ApolloLinkFormData",
  "engines": {
    "node": ">= 8 < 9 || >= 10"
  },
  "keywords": [
    "apollo-link",
    "form-data",
    "upload",
    "file-upload",
    "graphql"
  ],
  "browser": {
    "./lib/util/isFile": "./lib/util/isFileBrowser",
    "./lib/util/makeFetcherPayload": "./lib/util/makeFetcherPayloadBrowser"
  },
  "scripts": {
    "eslint": "eslint lib test",
    "staged": "lint-staged",
    "test": "ava",
    "coverage": "nyc npm run test",
    "report": "npm run coverage && nyc report --reporter=html npm run test",
    "ci": "npm run coverage && nyc report --reporter=lcov npm run test && codecov"
  },
  "ava": {
    "files": [
      "test/**/*.js",
      "!test/**/__helper__/*.js"
    ]
  },
  "devDependencies": {
    "@octetstream/eslint-config": "3.0.0",
    "ava": "1.4.1",
    "codecov": "3.3.0",
    "eslint": "5.16.0",
    "eslint-plugin-ava": "6.0.0",
    "fetch-mock": "7.3.1",
    "formdata-node": "1.5.2",
    "graphql": "14.2.1",
    "graphql-tag": "2.10.1",
    "husky": "1.3.1",
    "isomorphic-fetch": "2.2.1",
    "lint-staged": "8.1.5",
    "nyc": "14.0.0",
    "proxyquire": "2.1.0",
    "sinon": "7.3.2"
  },
  "peerDependencies": {
    "formdata-node": ">= 1.5 < 2",
    "graphql": "0.11.x || 0.12.x || 0.13.x || 14.0.2"
  },
  "dependencies": {
    "@octetstream/object-to-form-data": "1.1.0",
    "apollo-link": "1.2.11"
  }
}
